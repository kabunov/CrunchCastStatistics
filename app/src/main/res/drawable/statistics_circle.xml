<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:innerRadius="95dp"
			android:shape="ring"
			android:thickness="@dimen/circle_outer_border"
			android:useLevel="false">
			<solid
				android:color="@color/circle_outer">
			</solid>
			<size
				android:height="@dimen/circle_size"
				android:width="@dimen/circle_size">
			</size>
		</shape>
	</item>
	<item>
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:innerRadius="65dp"
			android:shape="ring"
			android:thickness="@dimen/circle_inner_width"
			android:useLevel="false">
			<solid
				android:color="@color/circle_inner">
			</solid>
			<size
				android:height="@dimen/circle_size"
				android:width="@dimen/circle_size">
			</size>
		</shape>
	</item>
	<item>
		<shape
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:innerRadius="60dp"
			android:shape="ring"
			android:thickness="5dp"
			android:useLevel="false">
			<solid
				android:color="@color/circle_outer">
			</solid>
			<size
				android:height="@dimen/circle_size"
				android:width="@dimen/circle_size">
			</size>
		</shape>
	</item>
</layer-list>